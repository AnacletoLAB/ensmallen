{
    "StringPPI": {
        "urls": [
            "https://stringdb-static.org/download/protein.actions.v11.0/9606.protein.actions.v11.0.txt.gz"
        ],
        "arguments": {
            "edge_path": "9606.protein.actions.v11.0.txt",
            "sources_column": "item_id_a",
            "destinations_column": "item_id_b",
            "edge_types_column": "mode",
            "weights_column": "score"
        }
    },
    "CompleteStringPPI": {
        "urls": [
            "https://stringdb-static.org/download/protein.links.v11.0.txt.gz"
        ],
        "arguments": {
            "edge_path": "protein.links.v11.0.txt",
            "sources_column": "protein1",
            "destinations_column": "protein2",
            "edge_separator": " ",
            "weights_column": "combined_score"
        }
    },
    "KG-COVID19": {
        "urls": [
            "https://kg-hub.berkeleybop.io/kg-covid-19/current/kg-covid-19.tar.gz"
        ],
        "arguments": {
            "edge_path": "kg-covid-19/merged-kg_edges.tsv",
            "node_path": "kg-covid-19/merged-kg_nodes.tsv",
            "sources_column": "subject",
            "destinations_column": "object",
            "edge_types_column": "edge_label",
            "nodes_column": "id",
            "node_types_column": "category",
            "default_node_type": "biolink:NamedThing"
        }
    },
    "YueStringPPI": {
        "urls": [
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/STRING_PPI/STRING_PPI.edgelist",
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/STRING_PPI/node_list.txt"
        ],
        "arguments": {
            "edge_path": "STRING_PPI.edgelist",
            "node_path": "node_list.txt",
            "edge_separator": " ",
            "sources_column_number": 0,
            "destinations_column_number": 1,
            "edge_header": false,
            "nodes_column": "STRING_id",
            "numeric_edge_node_ids": true
        }
    },
    "YueCTDDDA": {
        "urls": [
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/CTD_DDA/CTD_DDA.edgelist",
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/CTD_DDA/node_list.txt"
        ],
        "arguments": {
            "edge_path": "CTD_DDA.edgelist",
            "node_path": "node_list.txt",
            "edge_separator": " ",
            "sources_column_number": 0,
            "destinations_column_number": 1,
            "edge_header": false,
            "node_header": false,
            "node_rows_to_skip": 1,
            "nodes_column_number": 1,
            "node_types_column_number": 3,
            "numeric_edge_node_ids": true
        }
    },
    "YueDrugBankDDI": {
        "urls": [
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/DrugBank_DDI/DrugBank_DDI.edgelist",
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/DrugBank_DDI/node_list.txt"
        ],
        "arguments": {
            "edge_path": "DrugBank_DDI.edgelist",
            "node_path": "node_list.txt",
            "edge_separator": " ",
            "sources_column_number": 0,
            "destinations_column_number": 1,
            "edge_header": false,
            "nodes_column": "DrugBank_id",
            "numeric_edge_node_ids": true
        }
    },
    "YueNDFRTDDA": {
        "urls": [
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/NDFRT_DDA/NDFRT_DDA.edgelist",
            "https://raw.githubusercontent.com/xiangyue9607/BioNEV/master/data/NDFRT_DDA/node_list.txt"
        ],
        "arguments": {
            "edge_path": "NDFRT_DDA.edgelist",
            "node_path": "node_list.txt",
            "edge_separator": " ",
            "sources_column_number": 0,
            "destinations_column_number": 1,
            "edge_header": false,
            "node_header": false,
            "node_rows_to_skip": 1,
            "nodes_column_number": 1,
            "node_types_column_number": 2,
            "numeric_edge_node_ids": true
        }
    }
