{
    "metadata":{
        "libname":"ensmallen",
        "version":"0.7.0.dev17",
        "abi_tag":"abi3",
        "license":"MIT",
        "source_code_url":"https://github.com/LucaCappelletti94/ensmallen",
        "readme_path":"README.md",
        "keywords":["python", "rust", "graph", "random", "walk"],
        "deps":[
            "downloaders>=1.0.15",
            "compress_json>=1.0.6",
            "userinput>=1.0.16",
            "cache_decorator>=2.0.13",
            "tqdm",
            "pandas",
            "transformers",
            "py-cpuinfo",
            "environments_utils>=1.0.4"
        ],
        "authors":[
            {
                "name":"Luca Cappelleti",
                "email":"cappelletti.luca94@gmail.com"
            },
            {
                "name":"Tommaso Fontana",
                "email":"tommaso.fontana.96@gmail.com"
            }
        ]
    },
    "wheels_folder":"wheels", 
    "shared_rustflags":"-C inline-threshold=1000",
    "develop":{
        "target_cpu":"native",
        "crate_features":["cuda"]
    },
    "targets":{
        "x86_64":{
            "balanced_cuda":{
                "target_cpu":"haswell",
                "crate_features":["cuda"],
                "target_os":["linux"]
            },
            "balanced":{
                "target_cpu":"haswell"
            },
            "compatibility":{
                "target_cpu":"core2"
            }
        },
        "aarch64":{
            "mac_m1":{
                "target_cpu":"apple-m1",
                "target_os":["darwin"]
            }
        }
    }
}
